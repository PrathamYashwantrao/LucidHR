                                                        <h6 class="mb-0">@d.Fullname</h6>
                                                        <span>@d.Uemail</span>
                                                    </td>
                                                    <td><span>@d.Dob</span></td>
                                                    <td><span>@d.Contact</span></td>
                                                    <td>@d.Dateofjoining</td>
                                                    <td>@d.Designation</td>
